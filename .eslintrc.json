{
  "extends": ["eslint:recommended"],
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "globals": {},
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "no-console": 0,
    "sourceType": "module",

    "angular/angularelement": false,
    "angular/timeout-service": false,
    "angular/interval-service": false,
    "angular/window-service": false,
    "angular/document-service": false,
    "angular/log": false,
    "angular/controller-name": false
  }
}
